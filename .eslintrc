{
  "parser": "babel-eslint",
  "rules": {
    "camelcase": 0,
    "no-comma-dangle": "always",
    "quotes": [ 2, "single" ],
    "no-inner-declarations": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": [ 2, "nofunc" ],
    "strict": 0
  },
  "env": {
    "node": true
  }
}
